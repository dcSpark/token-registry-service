{
  "name": "token-registry-service",
  "version": "v0.2.66",
  "description": "Token registry service for Cardano",
  "main": "src/index.ts",
  "scripts": {
    "buildtsc": "tsc",
    "build": "NODE_ENV=production webpack --config ./webpack.production.config.ts",
    "postinstall": "npm run typecheck",
    "typecheck": "tsc --project tsconfig.typecheck.json",
    "start": "pm2 start pm2.yaml",
    "stop": "pm2 stop pm2.yaml",
    "dev:watch": "NODE_ENV=development webpack",
    "dev:run": "NODE_ENV=development nodemon build/index.js",
    "eslint": "eslint \"src/**/*.ts\" tests/**/*.ts",
    "test": "mocha -r ts-node/register tests/**/*.test.ts --slow 0"
  },
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "npm run typecheck"
    }
  },
  "dependencies": {
    "@emurgo/cip14-js": "3.0.1",
    "axios": "^0.21.4",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^5.0.0-alpha.8",
    "mocha": "^9.1.3",
    "pm2": "^5.1.2",
    "semver-compare": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/compression": "^1.7.2",
    "@types/config": "0.0.39",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.27",
    "@types/semver-compare": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^4.3.8",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.4",
    "typescript-is": "^0.18.3",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "engines": {
    "node": "14.17.6",
    "npm": "6.14.15"
  }
}
